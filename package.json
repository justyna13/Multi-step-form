{
  "name": "vite-project",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite --mode development",
    "build": "tsc && vite build --mode production",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "format": "prettier --write src/* --config ./.prettierrc.cjs",
    "test": "vitest -u run --silent",
    "test:watch": "vitest -u",
    "test:coverage": "vitest -u run --silent --coverage",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "cypress": "cypress open --e2e -b chrome"
  },
  "dependencies": {
    "@hello-pangea/dnd": "^16.3.0",
    "@hookform/resolvers": "^3.3.1",
    "axios": "^1.6.5",
    "date-fns": "^3.3.1",
    "i18next": "^23.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.46.1",
    "react-i18next": "^14.0.1",
    "react-router-dom": "^6.16.0",
    "vite-plugin-svgr": "^4.2.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@hookform/devtools": "^4.3.1",
    "@storybook/addon-essentials": "^7.6.9",
    "@storybook/addon-interactions": "^7.6.9",
    "@storybook/addon-links": "^7.6.9",
    "@storybook/addon-onboarding": "^1.0.10",
    "@storybook/blocks": "^7.6.9",
    "@storybook/react": "^7.6.9",
    "@storybook/react-vite": "^7.6.9",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.2.15",
    "@types/react-datepicker": "^4.15.2",
    "@types/react-dom": "^18.2.7",
    "@types/mocha": "^10.0.6",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-istanbul": "^1.2.0",
    "autoprefixer": "^10.4.15",
    "cypress": "^13.6.3",
    "daisyui": "^4.6.0",
    "eslint": "^8.45.0",
    "mocha": "^10.2.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-storybook": "^0.6.13",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.29",
    "prettier": "^3.0.3",
    "react-datepicker": "^5.0.0",
    "sass": "^1.69.4",
    "storybook": "^7.6.9",
    "typescript": "^5.0.2",
    "vite": "^5.0.12",
    "vitest": "^1.2.0"
  },
  "lint-staged": {
    "**/*.{js,ts,tsx,json,html}": [
      "prettier --write .",
      "eslint . --fix"
    ]
  }
}
